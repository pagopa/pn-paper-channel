# Abilita invio richieste all'OCR
PN_PAPERCHANNEL_ENABLEOCR=true

PN_PAPERCHANNEL_PAPERTRACKERENABLED=true

# La variabile PN_PAPERCHANNEL_PAPERTRACKERPRODUCTLIST deve avere come formato il CommaDelimitedList
# Valori ammessi: [AR, RS, 890, RIR, RIS]
PN_PAPERCHANNEL_PAPERTRACKERPRODUCTLIST=AR

PN_PAPERCHANNEL_MAXPCRETRY=3

# Status code disabilitati al retry (Es. [RECRN006, CON996, ...]
PN_PAPERCHANNEL_DISABLEDRETRYSENDENGAGESTATUSCODES=